    "@mui/material": "^5.15.7",
    "@mui/x-date-pickers": "^6.19.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
