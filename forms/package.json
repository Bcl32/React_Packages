{
  "name": "@bcl32/forms",
  "version": "2.1.2",
  "description": "Forms to add new entities to the databases",
  "author": "",
  "license": "ISC",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./AddModelForm": {
      "types": "./dist/AddModelForm.d.ts",
      "import": "./dist/AddModelForm.js"
    },
    "./EditModelForm": {
      "types": "./dist/EditModelForm.d.ts",
      "import": "./dist/EditModelForm.js"
    },
    "./DeleteModelForm": {
      "types": "./dist/DeleteModelForm.d.ts",
      "import": "./dist/DeleteModelForm.js"
    },
    "./FormElement": {
      "types": "./dist/FormElement.d.ts",
      "import": "./dist/FormElement.js"
    },
    "./ButtonDatePicker": {
      "types": "./dist/ButtonDatePicker.d.ts",
      "import": "./dist/ButtonDatePicker.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
