{
  "name": "@bcl32/datatable",
  "version": "2.1.1",
  "description": "Custom data table built using react query table",
  "author": "",
  "license": "ISC",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./DataTable": {
      "types": "./dist/DataTable.d.ts",
      "import": "./dist/DataTable.js"
    },
    "./Table": {
      "types": "./dist/Table.d.ts",
      "import": "./dist/Table.js"
    },
    "./KeyValueTable": {
      "types": "./dist/KeyValueTable.d.ts",
      "import": "./dist/KeyValueTable.js"
    },
    "./StatsTable": {
      "types": "./dist/StatsTable.d.ts",
      "import": "./dist/StatsTable.js"
    },
    "./ColumnGenerator": {
      "types": "./dist/ColumnGenerator.d.ts",
      "import": "./dist/ColumnGenerator.js"
    },
    "./RowActions": {
      "types": "./dist/RowActions.d.ts",
      "import": "./dist/RowActions.js"
    },
    "./TablePagination": {
      "types": "./dist/TablePagination.d.ts",
      "import": "./dist/TablePagination.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Bcl32/React_Packages.git",
    "directory": "datatable"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "dayjs": "^1.11.10"
  },
  "dependencies": {
    "@bcl32/utils": "workspace:^2.0.0",
    "@bcl32/data-utils": "workspace:^2.0.0",
    "@bcl32/hooks": "workspace:^2.0.0",
    "@bcl32/forms": "workspace:^2.0.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@tanstack/react-table": "^8.11.8",
    "@mui/icons-material": "^5.15.7",
    "@mui/material": "^5.15.7",
    "react-router-dom": "^6.22.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
